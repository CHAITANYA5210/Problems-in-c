#include <stdio.h>
int main()
{
    int total;
    printf("how many elements are there in the array\n");
    scanf("%d",&total);
    int array[total];
    for(int i=0;total>i;i++)
    {
        printf("what is the number\n");
        scanf("%d",&array[i]);
    }
    int temp=array[0];
    for(int i=0;total>i;i++)
    {
        if(array[i]<temp)
        {
            temp=array[i];
        }
    }
    printf("%d is the smallest",temp);
}
