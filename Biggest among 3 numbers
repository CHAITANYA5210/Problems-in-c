main()
{
    int a,b,c;
    printf("what is a?\n");
    scanf("%d",&a);
    printf("what is b?\n");
    scanf("%d",&b);
    printf("what is c?\n");
    scanf("%d",&c);
    if(a>b && a>c )
    {
        printf("%d is greater",a);
    }
    else
    {
        if(b>a,b>c)
        {
            printf("%d is greater",b);
        }
        else
        {
            printf("%d is greater",c);
        }
    }
}
